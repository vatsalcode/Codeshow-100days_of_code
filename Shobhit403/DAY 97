#include<bits/stdc++.h>
using namespace std ;
int main()
{
   int t;
   cin>>t;
   for(int i=0;i<t;i++)
   {
      int size,k=0;
      cin>>size>>k;
      int arr[size],arr_1[size];
      for(int j=0;j<size;j++)
      {
          cin>>arr[j];
      }
      for(int j=0;j<size;j++)
      {
          if(k%arr[j]==0)
          {
              arr_1[j]=arr[j];
          }
          else
          {
              arr_1[j]=-1;
          }
      }
      sort(arr_1,arr_1+size);
      cout<<arr_1[size-1]<<endl;
   }
     return 0;
}
